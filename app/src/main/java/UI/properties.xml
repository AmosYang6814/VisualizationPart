<?xml version="1.0" encoding="UTF-8" ?>

<UI>
    <AttributeDefine>
        <module  name="all">
        </module>

        <module name="android.widget.EditText">
            <attribute name="输入"
                inputTypeform="(String)"
                reflectMethod="setText"
                Passingform="(#,null)"/>
        </module>

        <module name="android.widget.Button">
            <attribute name="按钮显示"
                inputTypeform="(String)"
                reflectMethod="setText"
                Passingform="(#)"/>

            <attribute name="背景颜色"
                inputTypeform="(Color)"
                reflectMethod="setBackgroundColor"
                Passingform="(#)"
            />

        </module>


    </AttributeDefine>
</UI>